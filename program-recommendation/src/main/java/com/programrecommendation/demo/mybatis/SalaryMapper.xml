<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.programrecommendation.demo.mapper.SalaryMapper">
	<select id="getAllSalary" resultType="SalaryModel">
		SELECT * FROM SALARY;
	</select>

	<select id="getSalary" resultType="SalaryModel">
		SELECT * FROM SALARY Where id = #{id}
	</select>

</mapper>